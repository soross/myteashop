<ehcache>
    <diskStore path="java.io.tmpdir"/>

      <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="15"
        timeToLiveSeconds="15"
        overflowToDisk="true" />

    <!-- dhsCache cache-->
	<cache name="powerCache"
		   maxElementsInMemory="10000"
           eternal="false"
           timeToIdleSeconds="15"
           timeToLiveSeconds="15"
           overflowToDisk="false" />           
          
</ehcache>